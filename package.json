{
  "name": "bookmarks",
  "version": "0.0.1",
  "description": "Bookmarks app for React.js, redux, graphql, Apollo, Node.js, mongodb, Mongoose",
  "main": "index.js",
  "scripts": {
    "start": "echo start",
    "server": "nodemon --exec babel-node ./server/index.js",
    "file-server": "node ./file_server",
    "build:globals": "webpack --config ./compiler/webpack.config.js",
    "build:dev": "npm run build:globals && WEBPACK_CONFIG_TYPE=dev webpack --config ./compiler/webpack.config.js",
    "build:prod": "NODE_ENV=production npm run build:globals && NODE_ENV=production WEBPACK_CONFIG_TYPE=prod webpack --config ./compiler/webpack.config.js",
    "build:hot": "WEBPACK_CONFIG_TYPE=hot webpack --config ./compiler/webpack.config.js",
    "client": "npm-run-all --parallel file-server build:dev",
    "client:hot": "WEBPACK_CONFIG_TYPE=hot npm-run-all --parallel file-server build:hot"
  },
  "keywords": [
    "React.js",
    "redux",
    "create-redux-store",
    "graphql",
    "Apollo",
    "Node.js",
    "mongodb",
    "Mongoose",
    "styled-components"
  ],
  "author": "nerjs",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nerjs/bookmark"
  },
  "nodemonConfig": {
    "ignore": [
      "src/*",
      "node_modules/*"
    ],
    "delay": "1500",
    "env": {
      "NODE_ENV": "development",
      "NODE_PATH": "./server/lib"
    }
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.5.1",
    "npm-run-all": "^4.1.3",
    "opn": "^5.3.0",
    "react-hot-loader": "^4.3.3",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.22.3"
  },
  "dependencies": {
    "apollo-boost": "^0.1.13",
    "apollo-cache-inmemory": "^1.2.7",
    "apollo-client": "^2.3.8",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-state": "^0.4.1",
    "apollo-server-express": "^1.4.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.5",
    "colors": "^1.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "create-redux-store": "^1.1.2",
    "crypto": "^1.0.1",
    "css-loader": "^0.28.9",
    "css-mqpacker": "^6.0.2",
    "cssnano": "^3.10.0",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "dotenv-webpack": "^1.5.7",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-polyfill": "^0.8.2",
    "formik": "^1.0.2",
    "graphql": "^0.13.2",
    "graphql-middleware": "^1.5.0",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.5",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.5",
    "merge-graphql-schemas": "^1.5.3",
    "mongoose": "^5.2.4",
    "morgan": "^1.9.0",
    "node-static": "^0.7.10",
    "nodemon": "^1.18.3",
    "normalize.css": "^8.0.0",
    "postcss-apply": "^0.8.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-discard-duplicates": "^2.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.0",
    "prop-types": "^15.6.1",
    "query-string": "^6.0.0",
    "react": "^16.2.0",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "style-loader": "^0.20.1",
    "styled-components": "^3.3.3",
    "uglifyjs-webpack-plugin": "^2.0.0",
    "webpack": "^3.12.0",
    "yup": "^0.26.0"
  }
}
